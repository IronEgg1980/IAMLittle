<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true" android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:topRightRadius="@dimen/genderCheckboxRadius"
                android:bottomRightRadius="@dimen/genderCheckboxRadius"/>
            <solid android:color="@color/colorWoman"/>
        </shape>
    </item>
    <item android:state_enabled="true" android:state_checked="false">
        <shape android:shape="rectangle">
            <stroke android:width="1dp"
                android:dashWidth="0dp"
                android:color="@color/colorWoman"/>
            <corners android:topRightRadius="@dimen/genderCheckboxRadius"
                android:bottomRightRadius="@dimen/genderCheckboxRadius"/>
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dp"
                android:dashWidth="0dp"
                android:color="@color/colorWoman"/>
            <corners android:topRightRadius="@dimen/genderCheckboxRadius"
                android:bottomRightRadius="@dimen/genderCheckboxRadius"/>
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
</selector>